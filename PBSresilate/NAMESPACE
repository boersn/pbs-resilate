# PBSresilate

# Import all packages listed as Imports or Depends
import(
  PBSmodelling, PBSddesolve, deSolve, rgl
)
importFrom("grDevices", "colorRampPalette")
importFrom("graphics", "box", "hist", "pairs", "par", "points", "rect", "segments")

# Export all functions that do NOT begin with a dot (period)
exportPattern("^[^\\.]")

# Export dot functions by action (explicit list just to keep track)
exportPattern("^\\.PBS")
exportPattern("^\\.calc")
exportPattern("^\\.get")
exportPattern("^\\.on")
exportPattern("^\\.plot")


